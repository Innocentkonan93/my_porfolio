{
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|eot|otf|ttf|ttc|woff|woff2|font.css)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=604800"
                    }
                ]
            },
            {
                "source": "**/*.@(js|css)",
                "headers": [
                    {
                        "key": "Content-Encoding",
                        "value": "gzip"
                    }
                ]
            }
        ],
        "cleanUrls": true,
        "trailingSlash": false
    },
    "flutter": {
        "platforms": {
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "innocent-konan",
                    "configurations": {
                        "web": "1:42922637893:web:10709e729b5b75b1b0412c"
                    }
                }
            }
        }
    }
}